{
  "workbench.editor.showTabs": "single",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 1,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.activityBar.location": "hidden",
  "workbench.editor.splitSizing": "split",
  "workbench.colorTheme": "GitHub Dark Default",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.iconTheme": "eq-material-theme-icons-light",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.colorCustomizations": {
    "editorInlayHint.background": "#00000000",
    "editorInlayHint.foreground": "#808080",
    "scrollbar.shadow": "#ffffff00"
  },
  "window.menuBarVisibility": "toggle",
  "window.confirmBeforeClose": "keyboardOnly",
  "window.commandCenter": true,
  "breadcrumbs.filePath": "off",
  "breadcrumbs.symbolPath": "on",
  "debug.toolBarLocation": "commandCenter",
  "debug.showVariableTypes": true,
  "debug.javascript.enableNetworkView": true,
  "editor.suggestSelection": "first",
  "editor.fontLigatures": true,
  "editor.minimap.enabled": false,
  "editor.smoothScrolling": true,
  "editor.wordWrap": "on",
  "editor.formatOnSave": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.fontFamily": "'Cascadia Code', Consolas, 'Courier New', monospace",
  "editor.hover.above": false,
  "editor.inlineSuggest.enabled": true,
  "editor.suggest.matchOnWordStartOnly": false,
  "editor.scrollBeyondLastLine": false,
  "editor.inlayHints.padding": true,
  "editor.lineNumbers": "relative",
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "keyboard.dispatch": "keyCode",
  "terminal.integrated.tabs.enabled": false,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.shellIntegration.enabled": true,
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.commandsToSkipShell": [
    "workbench.action.focusStatusBar",
    "workbench.action.toggleAuxiliaryBar"
  ],
  "zenMode.fullScreen": false,
  "diffEditor.renderSideBySide": true,
  "git.allowForcePush": true,
  "git.confirmForcePush": true,
  "git.closeDiffOnOperation": true,
  "chat.agent.enabled": true,
  "chat.implicitContext.enabled": {
    "panel": "never",
    "editing-session": "never"
  },
  "nix.enableLanguageServer": true,
  "nix.serverPath": "nixd",
  "github.copilot.chat.edits.temporalContext.enabled": true,
  "github.copilot.chat.search.semanticTextResults": true,
  "github.copilot.nextEditSuggestions.enabled": true,
  "inlineChat.enableV2": true,
  "inlineChat.hideOnRequest": true,
  "vim.enableNeovim": true,
  "vim.neovimPath": "nvim",
  "vim.easymotion": true,
  "vim.useSystemClipboard": true,
  "vim.replaceWithRegister": true,
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-b>": false,
    "<C-p>": false
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "s"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "s"
      ]
    },
    {
      "before": [
        "<leader>",
        "c",
        "c"
      ],
      "commands": [
        "workbench.action.chat.openInEditor"
      ]
    },
    // Inspired by https://zed.dev/docs/vim
    {
      "before": [
        "g",
        "y"
      ],
      "commands": [
        "editor.action.goToTypeDefinition"
      ],
      "silent": true
    },
    {
      "before": [
        "c",
        "d"
      ],
      "commands": [
        "editor.action.rename"
      ],
      "silent": true
    },
    {
      "before": [
        "g",
        "A"
      ],
      "commands": [
        "editor.action.referenceSearch.trigger"
      ],
      "silent": true
    },
    {
      "before": [
        "g",
        "s"
      ],
      "commands": [
        "workbench.action.gotoSymbol"
      ],
      "silent": true
    },
    {
      "before": [
        "g",
        "S"
      ],
      "commands": [
        "workbench.action.showAllSymbols"
      ],
      "silent": true
    },
    {
      "before": [
        "g",
        "]"
      ],
      "commands": [
        "editor.action.marker.next"
      ],
      "silent": true
    },
    {
      "before": [
        "g",
        "["
      ],
      "commands": [
        "editor.action.marker.prev"
      ],
      "silent": true
    },
    {
      "before": [
        "g",
        "."
      ],
      "commands": [
        "editor.action.quickFix"
      ],
      "silent": true
    },
    {
      "before": [
        "]",
        "c"
      ],
      "commands": [
        "editor.action.dirtydiff.next"
      ],
      "silent": true
    },
    {
      "before": [
        "[",
        "c"
      ],
      "commands": [
        "editor.action.dirtydiff.previous"
      ],
      "silent": true
    },
    {
      "before": [
        "d",
        "o"
      ],
      "commands": [
        "git.openChange"
      ],
      "silent": true
    },
    {
      "before": [
        "d",
        "u"
      ],
      "commands": [
        "git.stageSelectedRanges"
      ],
      "silent": true
    },
    {
      "before": [
        "d",
        "U"
      ],
      "commands": [
        "git.unstageSelectedRanges"
      ],
      "silent": true
    },
    {
      "before": [
        "d",
        "p"
      ],
      "commands": [
        "git.revertSelectedRanges",
        "closeQuickDiff"
      ],
      "silent": true
    },
    {
      "before": [
        "g",
        "/"
      ],
      "commands": [
        "search.action.openEditor"
      ],
      "silent": true
    },
    {
      "before": [
        "<C-w>",
        "g",
        "d"
      ],
      "commands": [
        "editor.action.revealDefinitionAside"
      ],
      "silent": true
    },
    {
      "before": [
        "<C-w>",
        "l"
      ],
      "commands": [
        "workbench.action.toggleEditorGroupLock"
      ],
      "silent": true
    },
    {
      "before": [
        "<C-w>",
        "H"
      ],
      "commands": [
        "workbench.action.moveActiveEditorGroupLeft"
      ],
      "silent": true
    },
    {
      "before": [
        "<C-w>",
        "J"
      ],
      "commands": [
        "workbench.action.moveActiveEditorGroupDown"
      ],
      "silent": true
    },
    {
      "before": [
        "<C-w>",
        "K"
      ],
      "commands": [
        "workbench.action.moveActiveEditorGroupUp"
      ],
      "silent": true
    },
    {
      "before": [
        "<C-w>",
        "L"
      ],
      "commands": [
        "workbench.action.moveActiveEditorGroupRight"
      ],
      "silent": true
    },
    {
      "before": [
        "<C-w>",
        "_"
      ],
      "commands": [
        "workbench.action.minimizeOtherEditors"
      ],
      "silent": true
    },
    {
      "before": [
        "<C-w>",
        "z"
      ],
      "commands": [
        "workbench.action.toggleMaximizeEditorGroup"
      ],
      "silent": true
    },
    {
      "before": [
        "u"
      ],
      "commands": [
        "undo"
      ],
      "silent": true
    },
    {
      "before": [
        "<C-r>"
      ],
      "commands": [
        "redo"
      ],
      "silent": true
    },
    {
      "before": [
        "g",
        "h"
      ],
      "after": []
    }
  ],
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<C-x>",
        "<C-a>"
      ],
      "commands": [
        "inlineChat.start"
      ],
      "silent": true
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    }
  ],
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  }
}